{
  "id": "premium-fitness-dashboard",
  "title": "Premium Fitness Tracking Dashboard",
  "description": "A premium, visually stunning fitness tracking dashboard with monochrome blue gradients, shimmer loading effects, and geometric pattern overlays.",
  "category": "Healthcare",
  "color_palette": {
    "primary": "#0A192F",
    "secondary": "#1E3A8A",
    "accent": "#00FFA3",
    "background": "#E0F7FA",
    "text": "#FFFFFF"
  },
  "layout": {
    "type": "card-based-grid",
    "structure": [
      "header",
      "main",
      "sidebar",
      "footer"
    ]
  },
  "components": {
    "shadcn": [
      "Button",
      "Card",
      "Input",
      "Select",
      "Table",
      "Dialog",
      "Form",
      "Badge",
      "Avatar",
      "Tabs",
      "Progress",
      "Skeleton",
      "Command",
      "Collapsible",
      "Separator"
    ],
    "radix": [
      "Dialog",
      "DropdownMenu",
      "Popover",
      "Tabs",
      "Switch",
      "Slider",
      "ScrollArea",
      "RadioGroup"
    ]
  },
  "features": [
    "shimmer-loading",
    "geometric-overlays",
