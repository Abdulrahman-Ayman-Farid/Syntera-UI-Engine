  "category": "Developer",
  "color_palette": {
    "primary": "#8BC34A",
    "secondary": "#CDDC39",
    "accent": "#FFEB3B",
    "background": "#F5F5DC",
    "text": "#2E7D32"
  },
  "layout": {
    "type": "top-navigation-bar",
    "structure": [
      "header",
      "main",
      "footer"
    ]
  },
  "components": {
