      "branches": "number",
      "stars": "number",
      "forks": "number",
      "lastCommit": "Date"
    },
    "Commit": {
      "id": "string",
      "message": "string",
      "author": "string",
      "date": "Date",
      "sha": "string",
      "filesChanged": "number"
    },
    "Branch": {
