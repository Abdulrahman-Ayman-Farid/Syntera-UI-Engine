      "repositories": "Repository[]",
      "currentRepo": "Repository | null",
      "commits": "Commit[]",
      "branches": "Branch[]",
      "activeBranch": "string"
    },
    "queries": [
      "fetchRepositories",
      "fetchCommits",
      "fetchBranches",
