{
  "id": "cryptocurrency-exchange-template",
  "title": "Premium Cryptocurrency Exchange Dashboard",
  "description": "A visually stunning and highly functional dashboard for a cryptocurrency exchange, designed with an industrial theme and premium aesthetics.",
  "category": "finance",
  "color_palette": {
    "primary": "#333333",
    "secondary": "#FFA500",
    "accent": "#B22222",
    "background": "#1E1E1E",
    "text": "#FFFFFF"
  },
  "layout": {
    "type": "floating-action-sidebar",
    "structure": [
      "header",
      "main",
      "sidebar",
      "footer"
    ]
