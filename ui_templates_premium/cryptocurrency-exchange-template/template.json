  "components": {
    "shadcn": [
      "Button",
      "Card",
      "Input",
      "Select",
      "Table",
      "Dialog",
      "Form",
      "Badge",
      "Avatar",
      "Tabs",
      "Progress",
      "Skeleton",
      "Command",
      "Collapsible"
    ],
    "radix": [
      "Dialog",
      "DropdownMenu",
      "Popover",
      "Tabs",
      "Switch",
      "Slider",
      "ScrollArea",
      "RadioGroup"
    ]
  },
  "features": [
    "fade-in-on-scroll",
