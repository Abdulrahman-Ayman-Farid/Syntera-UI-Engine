{
  "id": "bug-tracking-tool-template",
  "title": "Premium Bug Tracking Tool Interface",
  "description": "A visually stunning and premium bug tracking tool interface designed with deep ocean themes, rich gradients, and advanced UI components.",
  "category": "Developer",
  "color_palette": {
    "primary": "#1E3A8A",
    "secondary": "#2A4D8B",
    "accent": "#3ECF8E",
    "background": "#152642",
    "text": "#FFFFFF"
  },
  "layout": {
    "type": "floating-action-sidebar",
    "structure": [
      "header",
      "main",
      "sidebar",
      "footer"
    ]
  },
  "components": {
    "shadcn": [
      "Button",
      "Card",
      "Input",
      "Select",
      "Table",
      "Dialog",
      "Form",
      "Badge",
      "Avatar",
      "Tabs",
      "Progress",
      "Skeleton"
    ],
    "radix": [
      "Dialog",
      "DropdownMenu",
      "Popover",
      "Tabs",
      "Switch",
      "Slider"
    ]
