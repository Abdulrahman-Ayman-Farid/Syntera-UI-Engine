{
  "id": "ecommerce-admin-dashboard",
  "title": "Modern E-commerce Admin Dashboard",
  "description": "Enterprise e-commerce administration platform with advanced product management, real-time order tracking, inventory optimization, sales analytics, and customer insights.",
  "category": "E-commerce",
  "color_palette": {
    "primary": "#2563eb",
    "secondary": "#3b82f6",
    "accent": "#8b5cf6",
    "background": "#f8fafc",
    "text": "#1f2937",
    "success": "#10b981",
    "warning": "#f59e0b",
    "danger": "#ef4444"
  },
  "layout": {
    "type": "ecommerce-dashboard",
    "structure": [
      "header",
      "sales-overview",
      "product-management",
      "order-tracking",
      "inventory-status",
      "customer-insights",
      "quick-actions"
    ]
  },
  "components": {
    "shadcn": [
      "Button",
      "Card",
      "Input",
      "Select",
      "Table",
      "Dialog",
      "Badge",
      "Progress",
      "Skeleton",
      "Tabs",
      "Tooltip",
      "Separator"
    ],
    "radix": [
      "Dialog",
      "Select",
      "Tabs",
      "Tooltip",
      "DropdownMenu"
    ],
    "semantic_roles": {
      "sales-kpi": "sales-overview",
      "product-card": "product-management",
      "order-row": "order-tracking",
      "inventory-metric": "inventory-status",
      "customer-stat": "customer-insights",
      "action-button": "quick-actions"
    }
  },
  "libraries_to_be_imported": {
    "charts": ["recharts"],
    "animations": ["framer-motion"],
    "icons": ["lucide-react"],
    "tables": ["@tanstack/react-table"],
    "utils": ["clsx", "tailwind-merge", "date-fns", "zod"]
  },
  "features": [
    "product-management",
    "order-fulfillment",
    "inventory-tracking",
    "sales-analytics",
    "customer-management",
    "bulk-operations",
    "real-time-updates",
    "export-reports"
  ],
  "responsive": {
    "breakpoints": {
      "xs": 0,
      "sm": 640,
      "md": 768,
      "lg": 1024,
      "xl": 1280,
      "2xl": 1536
    },
    "grid_config": {
      "sales_grid": { "xs": 1, "sm": 2, "md": 2, "lg": 4, "xl": 4, "2xl": 4 },
      "product_grid": { "xs": 1, "sm": 2, "md": 2, "lg": 3, "xl": 4, "2xl": 4 },
      "order_table": { "xs": 1, "sm": 1, "md": 1, "lg": 1, "xl": 1, "2xl": 1 }
    }
  },
  "accessibility": {
    "wcag_level": "AA",
    "contrast_modes": ["light", "dark"],
    "reduced_motion": true,
    "focus_variants": ["ring-2", "ring-blue-500", "ring-offset-2"],
    "keyboard_support": ["tab-navigation", "arrow-keys", "space-enter", "escape-key"],
    "screen_reader": {
      "live_regions": ["polite", "assertive"],
      "announcements": ["order-received", "stock-low", "product-updated"]
    }
  },
  "data_types": {
    "products": {
      "status": ["active", "draft", "archived", "out_of_stock"],
      "categories": ["electronics", "clothing", "home", "sports"],
      "formats": ["simple", "variable", "grouped", "digital"]
    },
    "orders": {
      "status": ["pending", "processing", "shipped", "delivered", "cancelled"],
      "payment_status": ["paid", "pending", "failed", "refunded"]
    },
    "charts": {
      "types": ["line", "bar", "pie"],
      "data_keys": ["revenue", "orders", "products", "customers"]
    }
  },
  "state_management": {
    "required_state": ["products", "orders", "filters", "isLoading"],
    "optional_state": ["selectedProducts", "bulkActions", "notifications"],
    "derived_state": ["totalRevenue", "lowStockItems", "pendingOrders"]
  },
  "ecommerce_features": {
    "product_operations": ["add", "edit", "delete", "bulk_update", "import_export"],
    "order_management": ["view", "update_status", "refund", "print_invoice"],
    "inventory_tracking": ["stock_levels", "low_stock_alerts", "restock_reminders"],
    "analytics": ["sales_trends", "top_products", "customer_behavior"]
  },
  "generation_hints": {
    "agent_instructions": "Use data-template-section for modular structure. Product table needs sorting, filtering, pagination. Orders require real-time status updates. Implement bulk operations for efficiency.",
    "extension_points": ["payment_gateways", "shipping_integrations", "inventory_systems", "crm_connectors"],
    "validation_rules": {
      "product_price": "Must be non-negative currency value",
      "stock_quantity": "Must be non-negative integer",
      "order_status": "Must be valid status from predefined list"
    }
  }
}